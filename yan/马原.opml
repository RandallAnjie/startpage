        <outline text="意识观" _mubu_text="%3Cspan%3E%E6%84%8F%E8%AF%86%E8%A7%82%3C/span%3E" _note="派生" _mubu_note="%3Cspan%3E%E6%B4%BE%E7%94%9F%3C/span%3E">
          <outline text="意识的来源、本质和作用—— #重点" _mubu_text="%3Cspan%20class=%22%20text-color-yellow%22%3E%E6%84%8F%E8%AF%86%E7%9A%84%E6%9D%A5%E6%BA%90%E3%80%81%E6%9C%AC%E8%B4%A8%E5%92%8C%E4%BD%9C%E7%94%A8%3C/span%3E%3Cspan%3E%E2%80%94%E2%80%94%20%3C/span%3E%3Cspan%20class=%22tag%22%3E#%E9%87%8D%E7%82%B9%3C/span%3E" _note="" _mubu_note="">
