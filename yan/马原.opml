          <outline text="认识过程的两次飞跃" _mubu_text="%3Cspan%3E%E8%AE%A4%E8%AF%86%E8%BF%87%E7%A8%8B%E7%9A%84%E4%B8%A4%E6%AC%A1%E9%A3%9E%E8%B7%83%3C/span%3E" _note="" _mubu_note="">
